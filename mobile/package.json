{
  "name": "sportbeacon-mobile",
  "version": "4.0.0",
  "private": true,
  "scripts": {
    "android": "react-native run-android",
    "ios": "react-native run-ios",
    "ios:simulator": "react-native run-ios --simulator='iPhone 15'",
    "ios:device": "react-native run-ios --device",
    "start": "react-native start",
    "test": "jest",
    "lint": "eslint . --ext .js,.jsx,.ts,.tsx",
    "lint:fix": "eslint . --ext .js,.jsx,.ts,.tsx --fix",
    "pod-install": "cd ios && pod install",
    "clean": "cd ios && xcodebuild clean && cd .. && npx react-native clean",
    "build:ios": "cd ios && xcodebuild -workspace SportBeaconAI.xcworkspace -scheme SportBeaconAI -configuration Release -destination generic/platform=iOS -archivePath build/SportBeaconAI.xcarchive archive",
    "test:ios": "cd ios && xcodebuild test -workspace SportBeaconAI.xcworkspace -scheme SportBeaconAI -destination 'platform=iOS Simulator,name=iPhone 15,OS=latest'"
  },
  "dependencies": {
    "react": "18.2.0",
    "react-native": "0.73.6",
    "@react-native-firebase/app": "^20.0.0",
    "@react-native-firebase/auth": "^20.0.0",
