<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE plist PUBLIC "-//Apple//DTD PLIST 1.0//EN" "http://www.apple.com/DTDs/PropertyList-1.0.dtd">
<plist version="1.0">
<dict>
	<key>CFBundleDevelopmentRegion</key>
	<string>en</string>
	<key>CFBundleDisplayName</key>
	<string>SportBeaconAI</string>
	<key>CFBundleExecutable</key>
	<string>$(EXECUTABLE_NAME)</string>
	<key>CFBundleIdentifier</key>
	<string>$(PRODUCT_BUNDLE_IDENTIFIER)</string>
	<key>CFBundleInfoDictionaryVersion</key>
	<string>6.0</string>
	<key>CFBundleName</key>
	<string>$(PRODUCT_NAME)</string>
	<key>CFBundlePackageType</key>
	<string>APPL</string>
	<key>CFBundleShortVersionString</key>
	<string>$(MARKETING_VERSION)</string>
	<key>CFBundleSignature</key>
	<string>????</string>
	<key>CFBundleVersion</key>
	<string>$(CURRENT_PROJECT_VERSION)</string>
	<key>LSRequiresIPhoneOS</key>
	<true/>
	<key>NSAppTransportSecurity</key>
	<dict>
		<key>NSExceptionDomains</key>
		<dict>
			<key>localhost</key>
			<dict>
				<key>NSExceptionAllowsInsecureHTTPLoads</key>
				<true/>
			</dict>
		</dict>
	</dict>
	<key>UILaunchStoryboardName</key>
	<string>LaunchScreen</string>
	<key>UIRequiredDeviceCapabilities</key>
	<array>
		<string>armv7</string>
	</array>
	<key>UISupportedInterfaceOrientations</key>
	<array>
		<string>UIInterfaceOrientationPortrait</string>
		<string>UIInterfaceOrientationLandscapeLeft</string>
		<string>UIInterfaceOrientationLandscapeRight</string>
	</array>
	<key>UIViewControllerBasedStatusBarAppearance</key>
	<false/>
	
	<!-- Camera Usage Description for Stripe Card Scanning and AR Features -->
	<key>NSCameraUsageDescription</key>
	<string>SportBeaconAI uses the camera to scan credit cards for payments and for augmented reality coaching features.</string>
	
	<!-- Microphone Usage Description for Voice Recognition -->
	<key>NSMicrophoneUsageDescription</key>
	<string>SportBeaconAI uses the microphone for voice commands and audio coaching features.</string>
	
	<!-- Location Usage Description for Venue Discovery -->
	<key>NSLocationWhenInUseUsageDescription</key>
	<string>SportBeaconAI uses your location to find nearby training venues and connect you with local coaches.</string>
	
	<!-- Bluetooth Usage Description for Wearables -->
	<key>NSBluetoothPeripheralUsageDescription</key>
	<string>SportBeaconAI uses Bluetooth to connect with fitness wearables and track your performance data.</string>
	
	<!-- Photo Library Usage Description -->
	<key>NSPhotoLibraryUsageDescription</key>
	<string>SportBeaconAI uses your photo library to save workout videos and share achievements.</string>
	
	<!-- Photo Library Add Usage Description -->
	<key>NSPhotoLibraryAddUsageDescription</key>
	<string>SportBeaconAI saves workout videos and achievements to your photo library.</string>
	
	<!-- Motion Usage Description for Fitness Tracking -->
	<key>NSMotionUsageDescription</key>
	<string>SportBeaconAI uses motion data to track your workout performance and provide personalized coaching.</string>
	
	<!-- Face ID Usage Description -->
	<key>NSFaceIDUsageDescription</key>
	<string>SportBeaconAI uses Face ID for secure login and payment authentication.</string>
	
	<!-- URL Schemes for Stripe 3D Secure -->
	<key>CFBundleURLTypes</key>
	<array>
		<dict>
			<key>CFBundleURLName</key>
			<string>com.sportbeaconai.payments</string>
			<key>CFBundleURLSchemes</key>
			<array>
				<string>sportbeaconai</string>
			</array>
		</dict>
	</array>
	
	<!-- Background Modes -->
	<key>UIBackgroundModes</key>
	<array>
		<string>remote-notification</string>
		<string>background-fetch</string>
		<string>background-processing</string>
		<string>audio</string>
		<string>location</string>
	</array>
	
	<!-- Firebase Configuration -->
	<key>FirebaseAppDelegateProxyEnabled</key>
	<false/>
	
	<!-- App Tracking Transparency -->
	<key>NSUserTrackingUsageDescription</key>
	<string>SportBeaconAI uses tracking to provide personalized coaching recommendations and improve your training experience.</string>
	
	<!-- Local Network Usage -->
	<key>NSLocalNetworkUsageDescription</key>
	<string>SportBeaconAI uses local network to discover nearby training partners and coaches.</string>
	
	<!-- Bonjour Services -->
	<key>NSBonjourServices</key>
	<array>
		<string>_sportbeacon._tcp</string>
		<string>_sportbeacon._udp</string>
	</array>
</dict>
</plist> 